        </div>
        <div class="slider-container">
          <div class="responsive_big" id="slider_2">
            <% data.bigCards.forEach(function(gigCard){ %>
              <div>
                <%- include('partials/card_slide_big', { 
                  imgUrl: gigCard.imgUrl,
                  author: gigCard.author,
                  date: gigCard.date,
                  title: gigCard.title, 
                  text: gigCard.text,
                  categories: gigCard.categories,
                  ttr: gigCard.ttr,
                  views: gigCard.views
                }); %>
              </div>
            <% }); %>
          </div>
        </div>
        <div class="flex-container" id="cat_card_container">
          <% data.smallCards.forEach(function(smallCard, cardIndex){ %>
            <%- include('partials/card_small', {
              index: cardIndex,
              show: 6,
              imgUrl: smallCard.imgUrl,
              author: smallCard.author,
              date: smallCard.date,
              title: smallCard.title, 
              text: smallCard.text,
              categories: smallCard.categories,
              ttr: smallCard.ttr,
              views: smallCard.views
            }); %>
          <% }); %>
        </div> 
        <div class="show-more-block">
          <a class="rounded-button show-more-trigger" data-target="#cat_card_container">Show more</a>
        </div>
        <div class="headline">
          <h2>You might also like</h2>
          <a href="featured.html" class="rounded-button_icon-right">
