      },
    },
    form: {
      id: 'restore-password-form',
      inputs : [
        { 
          class: 'input',
          type: 'email', 
          name: 'userEmail',
        },
        {
          class: 'input_button',
          type: 'submit',
          value: 'Apply',
        },
      ],
    },
  }); %>
  <%- include('partials/modal', { 
    id: 'modal_4',
